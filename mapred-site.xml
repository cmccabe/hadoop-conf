<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="configuration.xsl"?>
<configuration>
<property>
  <name>mapreduce.framework.name</name>
  <value>yarn</value>
</property>
<property>
  <name>mapred.task.tracker.http.address</name>
  <value>127.0.0.1:6400</value>
</property>
<property>
  <name>mapred.job.tracker</name>
  <value>127.0.0.1:6300</value>
</property>
<property>
  <name>mapred.local.dir</name>
  <value>_ROOT_/local1/mapred,_ROOT_/local2/mapred</value>
</property>
<property>
  <name>mapred.system.dir</name>
  <value>/mapred/system</value>
</property>
<property>
  <name>mapreduce.jobtracker.staging.root.dir</name>
  <value>/user/_USER_/mapred/staging</value>
</property>
<property>
  <name>mapred.temp.dir</name>
  <value>/user/_USER_/mapred/temp</value>
</property>
<property>
  <name>mapreduce.tasktracker.group</name>
  <value>mapred</value>
</property>
<property>
  <name>mapred.task.tracker.task-controller</name>
  <value>org.apache.hadoop.mapred.LinuxTaskController</value>
</property>
<property>
  <name>mapreduce.tasktracker.task-controller.exe</name>
  <value>_ROOT_/sbin/Linux-amd64-64/task-controller</value>
</property>
<!--
<property>
  <name>mapred.healthChecker.script.path</name>
  <value>_ROOT_/bin/tthealth.sh</value>
</property>
-->
</configuration>
