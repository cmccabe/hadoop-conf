<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="configuration.xsl"?>
<configuration xmlns:xi="http://www.w3.org/2001/XInclude">
  <!-- 
   A configuration file designed to be read by HA namenodes.
   Anything that needs my.ha.nn.id to be defined should go here.
  -->
  <property>
    <name>dfs.ha.namenode.id</name>
    <value>nn${my.ha.nn.id}</value>
  </property>
  <property>
    <name>dfs.name.dir</name>
    <value>file:///r/name${my.ha.nn.id}</value>
  </property>
</configuration>
